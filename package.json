{
  "name": "npm-scripts-nvm",
  "displayName": "npm-scripts-nvm",
  "description": "Extension to run npm scripts with nvm",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "publisher": "cloudlesslakitu",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:./package.json"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cloudless67/npm-scripts-nvm.git"
  },
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "npm-scripts-nvm.npmScripts",
          "name": "NPM Scripts",
          "when": "workspaceHasPackageJSON"
        }
      ]
    },
    "commands": [
      {
        "command": "npm-scripts-nvm.runNpmScript",
        "title": "Run NPM script"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.69.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "@vscode/test-electron": "^2.1.3"
  }
}
